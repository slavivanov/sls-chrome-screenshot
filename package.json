{
  "name": "aws",
  "private": true,
  "version": "1.0.0-55",
  "description": "Example serverless functions using the Serverless-framework",
  "main": "src/handlers.js",
  "engines": {
    "node": ">= 6.10.0"
  },
  "config": {
    "jsSrc": "src/",
    "chromiumChannel": "dev",
    "chromium_channel": "dev"
  },
  "scripts": {
    "test": "npm run lint && npm run ava",
    "watch:test": "ava --watch",
    "ava": "ava",
    "lint": "npm run lint:eslint -s",
    "lint:eslint": "eslint $npm_package_config_jsSrc",
    "deploy": "serverless deploy -v",
    "upgrade-dependencies": "yarn upgrade-interactive --latest --exact"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adieuadieu/serverless-chrome.git"
  },
  "keywords": [
    "serverless",
    "chrome",
    "chromium",
    "headless",
    "aws",
    "lambda",
    "serverless-framework",
    "screenshot",
    "screen capture",
    "pdf"
  ],
  "author": "Marco LÃ¼thy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adieuadieu/serverless-chrome/issues"
  },
  "homepage": "https://github.com/adieuadieu/serverless-chrome/tree/master/examples/serverless-framework/aws",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "chrome-aws-lambda": "~10.0.0",
    "chrome-remote-interface": "0.25.3",
    "crypto": "^1.0.1",
    "fs-extra": "^0.26.7",
    "puppeteer-core": "^10.1.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "ava": "0.23.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-object-entries": "1.0.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "copy-webpack-plugin": "^6.0.3",
    "imports-loader": "^3.0.0",
    "serverless": "1.24.1",
    "serverless-add-api-key": "^2.1.2",
    "serverless-prune-plugin": "^1.5.1",
    "serverless-webpack": "^5.5.1",
    "webpack": "^5.44.0",
    "webpack-node-externals": "^3.0.0"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherits"
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "12"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-object-entries"
    ]
  }
}
